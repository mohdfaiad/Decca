/* View: VIEW_JOB_COST_CAT_BY_JOB, Owner: SYSDBA */
CREATE VIEW VIEW_JOB_COST_CAT_BY_JOB (JOB_NO_1, JOB_NO_2, JOB_COST_CAT, JOB_COST_CAT_UNIT, REG_RATE, PREM_RATE, BUDGET_AMT, BUDGET_QTY, JOB_REG_QTY, JOB_REG_AMT, JOB_PREM_QTY, JOB_PREM_AMT) AS

   SELECT JOB_NO_1,
          JOB_NO_2,
          JOB_COST_CAT,
          JOB_COST_CAT_UNIT ,
          SUM(REG_RATE) ,
          SUM(PREM_RATE) ,
          SUM(BUDGET_AMT),
          SUM(BUDGET_QTY),
          SUM(JOB_REG_QTY) ,
          SUM(JOB_REG_AMT),
          SUM(JOB_PREM_QTY) ,
          SUM(JOB_PREM_AMT)
     FROM JOB_COST_CAT 
     GROUP BY JOB_NO_1,
              JOB_NO_2,
              JOB_COST_CAT,
              JOB_COST_CAT_UNIT

;
