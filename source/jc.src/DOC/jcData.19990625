CREATE TRIGGER JOB_DETAIL_INSERT FOR JOB_DETAIL                      
ACTIVE BEFORE INSERT POSITION 0 
as 
declare variable wJobNo1 char(4) character set big5;
declare variable wCostCatUnit char(2) character set big5;
declare variable wRegRate numeric(15,2);
declare variable wPremRate numeric(15,2);
declare variable wInt1 integer;
declare variable wInt2 integer;
declare variable wDate date;
begin

  wInt1 = new.Item_Unit_Cost;
  wInt2 = (new.Item_Unit_Cost - :wInt1) * 1000000;
  new.Item_Unit_Cost = :wInt1 + (:wInt2 / 1000000); 

  select Job_Cost_Cat_Unit,
         Reg_Rate,
         Prem_Rate
    from Job_Cost_File
    where Job_Cost_Cat = new.Job_Cost_Cat
    into :wCostCatUnit,
         :wRegRate,
         :wPremRate;

  wJobNo1 = null;
  select Job_No_1
    from Job_Cost_Cat
    where Job_No_1 = new.Job_No_1
      and Job_No_2 = new.Job_No_2
      and Sub_Job_No = new.Sub_Job_No
      and Job_Cost_Cat = new.Job_Cost_Cat
    into :wJobNo1;
  if (wJobNo1 is null) then
  begin
    if (new.Qty_Type = 'R') then
    begin
      insert into Job_Cost_Cat
        (Job_No_1, Job_No_2, Sub_Job_No,
         Reg_Rate, Prem_Rate,
         Budget_Qty, Budget_Amt,
         Job_Reg_Qty, Job_Prem_Qty,
         Draft_Budget_Qty, Draft_Budget_Amt,
         Job_Reg_Amt, Job_Prem_Amt,
         Job_Cost_Cat, Job_Cost_Cat_Unit)
      values
        (new.Job_No_1, new.Job_No_2, new.Sub_Job_No,
         :wRegRate, :wPremRate,
         0, 0,
         new.Detail_Qty, 0,
         0, 0,
         new.Detail_Amt, 0,
         new.Job_Cost_Cat, :wCostCatUnit);
    end
    if (new.Qty_Type <> 'R') then
    begin
      insert into Job_Cost_Cat
        (Job_No_1, Job_No_2, Sub_Job_No,
         Reg_Rate, Prem_Rate,
         Budget_Qty, Budget_Amt,
         Job_Reg_Qty, Job_Prem_Qty,
         Draft_Budget_Qty, Draft_Budget_Amt,
         Job_Reg_Amt, Job_Prem_Amt,
         Job_Cost_Cat, Job_Cost_Cat_Unit)
      values
        (new.Job_No_1, new.Job_No_2, new.Sub_Job_No,
         :wRegRate, :wPremRate,
         0, 0,
         0, new.Detail_Qty,
         0, 0,
         0, new.Detail_Amt,
         new.Job_Cost_Cat, :wCostCatUnit);
    end
  end
  if (wJobNo1 is not null) then
  begin
    if (new.Qty_Type = 'R') then
    begin
      update Job_Cost_Cat
        set Job_Reg_Qty = Job_Reg_Qty + new.Detail_Qty,
            Job_Reg_Amt = Job_Reg_Amt + new.Detail_Amt
        where Job_No_1 = new.Job_No_1
          and Job_No_2 = new.Job_No_2
          and Sub_Job_No = new.Sub_Job_No
          and Job_Cost_Cat = new.Job_Cost_Cat;
    end
    if (new.Qty_Type <> 'R') then
    begin
      update Job_Cost_Cat
        set Job_Prem_Qty = Job_Prem_Qty + new.Detail_Qty,
            Job_Prem_Amt = Job_Prem_Amt + new.Detail_Amt
        where Job_No_1 = new.Job_No_1
          and Job_No_2 = new.Job_No_2
          and Sub_Job_No = new.Sub_Job_No
          and Job_Cost_Cat = new.Job_Cost_Cat;
    end
  end

  wJobNo1 = null;
  select Job_No_1, First_Date
    from FWO_Cost_Cat
    where Job_No_1 = new.Job_No_1
      and Job_No_2 = new.Job_No_2
      and FWO_No = new.FWO_No
      and Job_Cost_Cat = new.Job_Cost_Cat
    into :wJobNo1,
         :wDate;
  if (wJobNo1 is null) then
  begin
    if (new.Qty_Type = 'R') then
    begin
      insert into FWO_Cost_Cat
        (Job_No_1, Job_No_2, FWO_No,
         Job_Cost_Cat, Job_Cost_Cat_Unit,
         Reg_Rate, Prem_Rate,
         Budget_Amt, Budget_Qty,
         Job_Reg_Qty, Job_Prem_Qty,
         Job_Reg_Amt, Job_Prem_Amt,
         First_Date)
      values
        (new.Job_No_1, new.Job_No_2, new.FWO_No,
         new.Job_Cost_Cat, :wCostCatUnit,
         :wRegRate, :wPremRate,
         0, 0,
         new.Detail_Qty, 0,
         new.Detail_Amt, 0,
         new.Hist_Date);
    end
    if (new.Qty_Type <> 'R') then
    begin
      insert into FWO_Cost_Cat
        (Job_No_1, Job_No_2, FWO_No,
         Job_Cost_Cat, Job_Cost_Cat_Unit,
         Reg_Rate, Prem_Rate,
         Budget_Amt, Budget_Qty,
         Job_Reg_Qty, Job_Prem_Qty,
         Job_Reg_Amt, Job_Prem_Amt,
         First_Date)
      values
        (new.Job_No_1, new.Job_No_2, new.FWO_No,
         new.Job_Cost_Cat, :wCostCatUnit,
         :wRegRate, :wPremRate,
         0, 0,
         0, new.Detail_Qty,
         0, new.Detail_Amt,
         new.Hist_Date);
    end
  end
  if (wJobNo1 is not null) then
  begin
    if (new.Hist_Date < :wDate) then
      wDate = new.Hist_Date;

    if (new.Qty_Type = 'R') then
    begin
      update FWO_Cost_Cat
        set Job_Reg_Qty = Job_Reg_Qty + new.Detail_Qty,
            Job_Reg_Amt = Job_Reg_Amt + new.Detail_Amt,
            First_Date = :wDate
        where Job_No_1 = new.Job_No_1
          and Job_No_2 = new.Job_No_2
          and FWO_No = new.FWO_No
          and Job_Cost_Cat = new.Job_Cost_Cat;
    end
    if (new.Qty_Type <> 'R') then
    begin
      update FWO_Cost_Cat
        set Job_Prem_Qty = Job_Prem_Qty + new.Detail_Qty,
            Job_Prem_Amt = Job_Prem_Amt + new.Detail_Amt,
            First_Date = :wDate
        where Job_No_1 = new.Job_No_1
          and Job_No_2 = new.Job_No_2
          and FWO_No = new.FWO_No
          and Job_Cost_Cat = new.Job_Cost_Cat;
    end
  end

  if (new.Qty_Type = 'R') then
    update Sub_Job_File
      set Job_Reg_Amt = Job_Reg_Amt + new.Detail_Amt
      where Job_No_1 = new.Job_No_1
        and Job_No_2 = new.Job_No_2
        and Sub_Job_No = new.Sub_Job_No;
  if (new.Qty_Type <> 'R') then
    update Sub_Job_File
      set Job_Prem_Amt = Job_Prem_Amt + new.Detail_Amt
      where Job_No_1 = new.Job_No_1
        and Job_No_2 = new.Job_No_2
        and Sub_Job_No = new.Sub_Job_No;
end
 ^
CREATE TRIGGER JOB_DETAIL_UPDATE FOR JOB_DETAIL                      
ACTIVE BEFORE UPDATE POSITION 0 
as 
declare variable wJobNo1 char(4) character set big5;
declare variable wCostCatUnit char(2) character set big5;
declare variable wRegRate numeric(15,2);
declare variable wPremRate numeric(15,2);
declare variable oRegQty numeric(15,2);
declare variable oRegAmt numeric(15,2);
declare variable oPremQty numeric(15,2);
declare variable oPremAmt numeric(15,2);
declare variable nRegQty numeric(15,2);
declare variable nRegAmt numeric(15,2);
declare variable nPremQty numeric(15,2);
declare variable nPremAmt numeric(15,2);
declare variable wInt1 integer;
declare variable wInt2 integer;
declare variable wDate Date;
begin
  if (new.Item_Unit_Cost <> old.Item_Unit_Cost) then
  begin
    wInt1 = new.Item_Unit_Cost;
    wInt2 = (new.Item_Unit_Cost - :wInt1) * 1000000;
    new.Item_Unit_Cost = :wInt1 + (:wInt2 / 1000000); 
  end

  if ((new.Qty_Type <> old.Qty_Type) or
     (new.Detail_Amt <> old.Detail_Amt) or
     (new.Detail_Qty <> old.Detail_Qty) or
     (new.Job_No_1 <> old.Job_No_1) or
     (new.Job_No_2 <> old.Job_No_2) or
     (new.Sub_Job_No <> old.Sub_Job_No) or
     (new.FWO_No <> old.FWO_No) or
     (new.Job_Cost_Cat <> old.Job_Cost_Cat)) then
  begin
    oRegQty = 0;
    oPremQty = 0;
    oRegAmt = 0;
    oPremAmt = 0;
    nRegQty = 0;
    nPremQty = 0;
    nRegAmt = 0;
    nPremAmt = 0;
    if (old.Qty_Type = 'R') then
    begin
      oRegQty = old.Detail_Qty;
      oRegAmt = old.Detail_Amt;
    end
    if (old.Qty_Type <> 'R') then
    begin
      oPremQty = old.Detail_Qty;
      oPremAmt = old.Detail_Amt;
    end
    if (new.Qty_Type = 'R') then
    begin
      nRegQty = new.Detail_Qty;
      nRegAmt = new.Detail_Amt;
    end
    if (new.Qty_Type <> 'R') then
    begin
      nPremQty = new.Detail_Qty;
      nPremAmt = new.Detail_Amt;
    end

    update Job_Cost_Cat
      set Job_Reg_Qty = Job_Reg_Qty - :oRegQty,
          Job_Reg_Amt = Job_Reg_Amt - :oRegAmt,
          Job_Prem_Qty = Job_Prem_Qty - :oPremQty,
          Job_Prem_Amt = Job_Prem_Amt - :oPremAmt
      where Job_No_1 = old.Job_No_1
        and Job_No_2 = old.Job_No_2
        and Sub_Job_No = old.Sub_Job_No
        and Job_Cost_Cat = old.Job_Cost_Cat;

    update FWO_Cost_Cat
      set Job_Reg_Qty = Job_Reg_Qty - :oRegQty,
          Job_Reg_Amt = Job_Reg_Amt - :oRegAmt,
          Job_Prem_Qty = Job_Prem_Qty - :oPremQty,
          Job_Prem_Amt = Job_Prem_Amt - :oPremAmt
      where Job_No_1 = old.Job_No_1
        and Job_No_2 = old.Job_No_2
        and FWO_No = old.FWO_No
        and Job_Cost_Cat = old.Job_Cost_Cat;

    update Sub_Job_File
      set Job_Reg_Amt = Job_Reg_Amt - :oRegAmt,
          Job_Prem_Amt = Job_Prem_Amt - :oPremAmt
      where Job_No_1 = old.Job_No_1
        and Job_No_2 = old.Job_No_2
        and Sub_Job_No = old.Sub_Job_No;

    update Sub_Job_File
      set Job_Reg_Amt = Job_Reg_Amt + :nRegAmt,
          Job_Prem_Amt = Job_Prem_Amt + :nPremAmt
      where Job_No_1 = new.Job_No_1
        and Job_No_2 = new.Job_No_2
        and Sub_Job_No = new.Sub_Job_No;

    select Job_Cost_Cat_Unit,
           Reg_Rate,
           Prem_Rate
      from Job_Cost_File
      where Job_Cost_Cat = new.Job_Cost_Cat
      into :wCostCatUnit,
           :wRegRate,
           :wPremRate;

    wJobNo1 = new.Job_No_1;
    if ((new.Job_No_1 <> old.Job_No_1) or
       (new.Job_No_2 <> old.Job_No_2) or
       (new.Sub_Job_No <> old.Sub_Job_No) or
       (new.Job_Cost_Cat <> old.Job_Cost_Cat)) then
    begin
      wJobNo1 = null;
      select Job_No_1
        from Job_Cost_Cat
        where Job_No_1 = new.Job_No_1
          and Job_No_2 = new.Job_No_2
          and Sub_Job_No = new.Sub_Job_No
          and Job_Cost_Cat = new.Job_Cost_Cat
        into :wJobNo1;
    end

    if (wJobNo1 is null) then
      insert into Job_Cost_Cat
        (Job_No_1, Job_No_2, Sub_Job_No,
         Reg_Rate, Prem_Rate,
         Budget_Qty, Budget_Amt,
         Job_Reg_Qty, Job_Prem_Qty,
         Draft_Budget_Qty, Draft_Budget_Amt,
         Job_Reg_Amt, Job_Prem_Amt,
         Job_Cost_Cat, Job_Cost_Cat_Unit)
      values
        (new.Job_No_1, new.Job_No_2, new.Sub_Job_No,
         :wRegRate, :wPremRate,
         0, 0,
         :nRegQty, :nPremQty,
         0, 0,
         :nRegAmt, :nPremAmt,
         new.Job_Cost_Cat, :wCostCatUnit);

    if (wJobNo1 is not null) then
      update Job_Cost_Cat
        set Job_Reg_Qty = Job_Reg_Qty + :nRegQty,
            Job_Reg_Amt = Job_Reg_Amt + :nRegAmt,
            Job_Prem_Qty = Job_Prem_Qty + :nPremQty,
            Job_Prem_Amt = Job_Prem_Amt + :nPremAmt
        where Job_No_1 = new.Job_No_1
          and Job_No_2 = new.Job_No_2
          and Sub_Job_No = new.Sub_Job_No
          and Job_Cost_Cat = new.Job_Cost_Cat;

    wJobNo1 = new.Job_No_1;
    if ((new.Job_No_1 <> old.Job_No_1) or
       (new.Job_No_2 <> old.Job_No_2) or
       (new.FWO_No <> old.FWO_No) or
       (new.Job_Cost_Cat <> old.Job_Cost_Cat)) then
    begin
      wJobNo1 = null;
      select Job_No_1, First_Date
        from FWO_Cost_Cat
        where Job_No_1 = new.Job_No_1
          and Job_No_2 = new.Job_No_2
          and FWO_No = new.FWO_No
          and Job_Cost_Cat = new.Job_Cost_Cat
        into :wJobNo1, 
             :wDate;
    end

    if (wJobNo1 is null) then
       insert into FWO_Cost_Cat
        (Job_No_1, Job_No_2, FWO_No,
         Job_Cost_Cat, Job_Cost_Cat_Unit,
         Reg_Rate, Prem_Rate,
         Budget_Amt, Budget_Qty,
         Job_Reg_Qty, Job_Prem_Qty,
         Job_Reg_Amt, Job_Prem_Amt,
         First_Date)
      values
        (new.Job_No_1, new.Job_No_2, new.FWO_No,
         new.Job_Cost_Cat, :wCostCatUnit,
         :wRegRate, :wPremRate,
         0, 0,
         :nRegQty, :nPremQty,
         :nRegAmt, :nPremAmt,
         new.Hist_Date);
    if (wJobNo1 is not null) then
    begin
      if (new.Hist_Date < :wDate) then
        wDate = new.Hist_Date;

      update FWO_Cost_Cat
        set Job_Reg_Qty = Job_Reg_Qty + :nRegQty,
            Job_Reg_Amt = Job_Reg_Amt + :nRegAmt,
            Job_Prem_Qty = Job_Prem_Qty + :nPremQty,
            Job_Prem_Amt = Job_Prem_Amt + :nPremAmt,
            First_Date = :wDate
        where Job_No_1 = new.Job_No_1
          and Job_No_2 = new.Job_No_2
          and FWO_No = new.FWO_No
          and Job_Cost_Cat = new.Job_Cost_Cat;
    end
  end
end
