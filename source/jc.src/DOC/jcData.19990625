ALTER PROCEDURE GET_JOB_PROGRESS (JOB_NO_1 CHAR(8),
JOB_NO_2 CHAR(2),
COST_CAT_1 CHAR(6),
COST_CAT_2 CHAR(6),
COST_CAT_3 CHAR(6),
COST_CAT_4 CHAR(6))
RETURNS (FWO_NO CHAR(4),
JOB_COST_CAT_UNIT_1 CHAR(4) CHARACTER SET BIG_5,
BUDGET_QTY_1 NUMERIC(15, 2),
NOW_FWO_QTY_1 DOUBLE PRECISION,
FIRST_DATE_1 DATE,
COMPLETE_PERCENT_1 NUMERIC(9, 2),
COMPLETE_DATE_1 DATE,
FWO_QTY_1 DOUBLE PRECISION,
JOB_COST_CAT_UNIT_2 CHAR(4) CHARACTER SET BIG_5,
BUDGET_QTY_2 NUMERIC(15, 2),
NOW_FWO_QTY_2 DOUBLE PRECISION,
FIRST_DATE_2 DATE,
COMPLETE_PERCENT_2 NUMERIC(9, 2),
COMPLETE_DATE_2 DATE,
FWO_QTY_2 DOUBLE PRECISION,
JOB_COST_CAT_UNIT_3 CHAR(4) CHARACTER SET BIG_5,
BUDGET_QTY_3 NUMERIC(15, 2),
NOW_FWO_QTY_3 DOUBLE PRECISION,
FIRST_DATE_3 DATE,
COMPLETE_PERCENT_3 NUMERIC(9, 2),
COMPLETE_DATE_3 DATE,
FWO_QTY_3 DOUBLE PRECISION,
JOB_COST_CAT_UNIT_4 CHAR(4) CHARACTER SET BIG_5,
BUDGET_QTY_4 NUMERIC(15, 2),
NOW_FWO_QTY_4 DOUBLE PRECISION,
FIRST_DATE_4 DATE,
COMPLETE_PERCENT_4 NUMERIC(9, 2),
COMPLETE_DATE_4 DATE,
FWO_QTY_4 DOUBLE PRECISION)
AS 
  
                
begin
  for 
    select Fwo_no  from job_fwo
    where job_no_1 = :job_no_1 and
          job_no_2 = :job_no_2
    into :Fwo_no
  
  do
    begin
      select Job_cost_cat_unit,
             budget_qty,
             (JOB_REG_QTY + JOB_PREM_QTY) now_Fwo_qty,   
             FIRST_DATE,
             COMPLETE_PERCENT,
             COMPLETE_DATE
      from fwo_cost_cat
      where job_no_1 = :job_no_1 and
            job_no_2 = :job_no_2 and
            Job_cost_cat = :cost_cat_1 and
            Fwo_no =:Fwo_no
      into :Job_cost_cat_unit_1,
           :budget_qty_1,
           :now_Fwo_qty_1,   
           :FIRST_DATE_1,
           :COMPLETE_PERCENT_1 ,
           :COMPLETE_DATE_1;
      select 
        sum(DETAIL_QTY) 
      from
        job_detail
      where job_no_1 = :job_no_1 and
            job_no_2 = :job_no_2 and
            Job_cost_cat = :cost_cat_1 and
            Fwo_no =:Fwo_no and
            hist_date <= :First_date_1
      into :Fwo_qty_1;

      select Job_cost_cat_unit,
             budget_qty,
             (JOB_REG_QTY + JOB_PREM_QTY) now_Fwo_qty,   
             FIRST_DATE,
             COMPLETE_PERCENT,
             COMPLETE_DATE
      from fwo_cost_cat
      where job_no_1 = :job_no_1 and
            job_no_2 = :job_no_2 and
            Job_cost_cat = :cost_cat_2 and
            Fwo_no =:Fwo_no
      into :Job_cost_cat_unit_2,
           :budget_qty_2,
           :now_Fwo_qty_2,   
           :FIRST_DATE_2,
           :COMPLETE_PERCENT_2 ,
           :COMPLETE_DATE_2;
      select 
        sum(DETAIL_QTY) 
      from
        job_detail
      where job_no_1 = :job_no_1 and
            job_no_2 = :job_no_2 and
            Job_cost_cat = :cost_cat_2 and
            Fwo_no =:Fwo_no and
            hist_date <= :First_date_2
      into :Fwo_qty_2;

      select Job_cost_cat_unit,
             budget_qty,
             (JOB_REG_QTY + JOB_PREM_QTY) now_Fwo_qty,   
             FIRST_DATE,
             COMPLETE_PERCENT,
             COMPLETE_DATE
      from fwo_cost_cat
      where job_no_1 = :job_no_1 and
            job_no_2 = :job_no_2 and
            Job_cost_cat = :cost_cat_3 and
            Fwo_no =:Fwo_no
      into :Job_cost_cat_unit_3,
           :budget_qty_3,
           :now_Fwo_qty_3,   
           :FIRST_DATE_3,
           :COMPLETE_PERCENT_3 ,
           :COMPLETE_DATE_3;
      select 
        sum(DETAIL_QTY) 
      from
        job_detail
      where job_no_1 = :job_no_1 and
            job_no_2 = :job_no_2 and
            Job_cost_cat = :cost_cat_3 and
            Fwo_no =:Fwo_no and
            hist_date <= :First_date_3
      into :Fwo_qty_3;

      select Job_cost_cat_unit,
             budget_qty,
             (JOB_REG_QTY + JOB_PREM_QTY) now_Fwo_qty,   
             FIRST_DATE,
             COMPLETE_PERCENT,
             COMPLETE_DATE
      from fwo_cost_cat
      where job_no_1 = :job_no_1 and
            job_no_2 = :job_no_2 and
            Job_cost_cat = :cost_cat_4 and
            Fwo_no =:Fwo_no
      into :Job_cost_cat_unit_4,
           :budget_qty_4,
           :now_Fwo_qty_4,   
           :FIRST_DATE_4,
           :COMPLETE_PERCENT_4 ,
           :COMPLETE_DATE_4;
      select 
        sum(DETAIL_QTY) 
      from
        job_detail
      where job_no_1 = :job_no_1 and
            job_no_2 = :job_no_2 and
            Job_cost_cat = :cost_cat_4 and
            Fwo_no =:Fwo_no and
            hist_date <= :First_date_4
      into :Fwo_qty_4;


      
      suspend;
    end     
end
